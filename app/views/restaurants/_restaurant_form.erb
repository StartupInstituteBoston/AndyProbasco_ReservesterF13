<%= form_for @restaurant do |f|%>
  <p>
    <%=f.label :name%>
    <%=f.text_field :name%>
  </p>
  <p>
    <%=f.label :description%>
    <%=f.text_area :description%>
  </p>
  <p>
    <%=f.label :address%>
    <%=f.text_field :address%>
  </p>
  <p>
    <%=f.label :phone_number%>
    <%=f.text_field :phone_number%>
  </p>
  <%=f.submit%>
<%end%>
